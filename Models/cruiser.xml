<!--Gear position-->
    <animation>
        <type>rotate</type>
        <object-name>gearFU</object-name>
        <object-name>gearFD</object-name>
        <object-name>wheelFL</object-name>
        <object-name>wheelFR</object-name>
        <property>gear/gear[0]/position-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind><dep>-92</dep>
            </entry>
            <entry>
                <ind>0.1</ind><dep>-90</dep>
            </entry>
            <entry>
                <ind>0.9</ind><dep>0</dep>
            </entry>
            <entry>
                <ind>1.0</ind><dep>0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>-6.44321</x-m>
            <y-m>0.0</y-m>
            <z-m>-1.04811</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>




